	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Conver To SQL From Json</Title>
                        <Shortcut></Shortcut>
			<Description>Conver To SQL From Json</Description>
			<Author>Brian Darley</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
<Declarations>
                                
			</Declarations>
			<Code Language="SQL"><![CDATA[
--ALTER DATABASE ITS_WSAPP_MASSMAIL SET COMPATIBILITY_LEVEL = 130
DECLARE @json NVARCHAR(MAX)

SET @json = 
N'[
	  {
		"primarySmtpAddress": "wjiang@email.unc.edu",
		"alias": "wjiang"
	  },
	  {
		"primarySmtpAddress": "diana.lee@unc.edu",
		"alias": "dalee"
	  }
  ]
'
SELECT *
into #tmp
FROM OPENJSON(@json)
WITH (   
    primarySmtpAddress   varchar(200) '$$.primarySmtpAddress' ,  
    alias     varchar(20)     '$$.alias'
) 


/*
select ',' +  c.name + ' ' + typ.name +
case when typ.name = 'nvarchar' then '(' + cast(c.max_length  as nvarchar) + ')' 
else '' 
end + ' ''$.' + c.name + ''''
 
 from sys.tables t
join sys.columns c on c.object_id = t.object_id
JOIN sys.types Typ ON TYP.user_type_id = c.user_type_id
where t.name = 'IdentityResourceClaims'
*/

]]>
			</Code>
